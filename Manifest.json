{
    "manifest_version": 3,
    "name": "Dark Mode LMS Sanata Dharma by 27xAlz",
    "description": "Unofficial Dark Mode untuk LMS website belajar Universitas Sanata Dharma by 27xAlz",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://belajar.usd.ac.id/*"],
            "js": ["Content.js"],
            "css": ["dark-mode.css"],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["dark-mode.css"],
            "matches": ["https://belajar.usd.ac.id/*"]
        }
    ]
}